{
    "name": "yellowtree/geoip-detect",
    "description": "Wordpress Plugin Geolocation IP Detection: Retrieving Geo-Information using the Maxmind GeoIP2 (Lite) Database.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Benjamin Pick (wp-geoip-detect posteo de)"
        }
    ],
    "keywords": [],
    "homepage": "http://www.yellowtree.de",
    "license": [
        "GPL-3.0-or-later"
    ],
    "require": {
        "php": ">=7.2.5",
        "geoip2/geoip2": "2.10.0",
        "maxmind-db/reader": "1.6.0",
        "maxmind/web-service-common": "0.7.0",
        "symfony/property-access": "5.4.2",
        "symfony/http-foundation": "5.4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.20",
        "roots/wordpress": "5.8.2",
        "wp-phpunit/wp-phpunit": "5.8.3",
        "yoast/phpunit-polyfills": "1.0.3"
    },
    "scripts": {
        "test": "phpunit",
        "install-test": "composer install --ignore-platform-reqs",
        "test-install": "composer install --ignore-platform-reqs && git update-index --assume-unchanged vendor/composer/autoload_* vendor/composer/installed.json"
    }
}
